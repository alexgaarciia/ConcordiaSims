library('dplyr')
library('pandas')
fn <- read_sav('fake_news.sav')
head(fn)
names(fn)
fn <- select (fn, !('RecipientLastName':'RecipientEmail'))
names(fn[,1:20])
fn[, c('AFFECTIVE_POLARIZATION')]
variable_labels <- sapply(fn, function(x) attr(x, "label"))
variable_table <- pandas.DataFrame(Column_Name = names(variable_labels), Variable_Label = variable_labels)
print(variable_table)
write.csv(variable_table, "questions.csv")
